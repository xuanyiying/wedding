{
  "name": "wedding-club-server",
  "version": "1.0.0",
  "description": "Wedding Club Official Website Backend API",
  "main": "dist/app.js",
  "scripts": {
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/app.ts",
    "build": "tsc",
    "build:scripts": "tsc -p tsconfig.json src/scripts/*.ts --outDir dist/scripts",
    "start": "node dist/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "db:init": "node dist/scripts/init-db.js"
  },
  "keywords": [
    "wedding",
    "club",
    "api",
    "nodejs",
    "express",
    "typescript"
  ],
  "author": "Wedding Club Team",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.844.0",
    "@aws-sdk/s3-request-presigner": "^3.846.0",
    "@types/express-slow-down": "^1.3.5",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/nanoid": "^2.1.0",
    "ali-oss": "^6.23.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.1.0",
    "express-validator": "^7.0.1",
    "fluent-ffmpeg": "^2.1.3",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "nodemailer": "^6.9.7",
    "sequelize": "^6.35.0",
    "sharp": "^0.34.3",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
    },
  "devDependencies": {
    "@types/ali-oss": "^6.16.11",
